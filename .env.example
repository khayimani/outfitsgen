# Environment Variables for OutfitsGen

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
S3_BUCKET_NAME=wardrobe-ingestion-bucket

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=wardrobe-index

# MongoDB Configuration
MONGO_URI=mongodb://localhost:27017/
MONGO_DB_NAME=wardrobe_db

# Frontend Configuration (for production deployment)
# NEXT_PUBLIC_BACKEND_URL=https://your-backend-url.vercel.app
